{
  "name": "@fullstacksjs/eslint-config",
  "version": "6.0.0",
  "license": "MIT",
  "author": "fullstacks <fullstacksjs@gmail.com>",
  "description": "fullstacks eslint config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstacksjs/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "rules",
    "base.js",
    "index.js",
    "jest.js",
    "react.js",
    "typescript.js",
    "typecheck.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "check-rules:base": "eslint-find-rules --unused ./base.js",
    "check-rules:jest": "eslint-find-rules --no-core --unused ./tests/jest.js",
    "check-rules:react": "eslint-find-rules --no-core --unused ./tests/react.js",
    "check-rules:typescript": "eslint-find-rules --no-core --unused ./tests/typescript.js",
    "test": "npm-run-all --parallel check-rules:*",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pre-commit": "npm run test && lint-staged"
  },
  "main": "index.js",
  "dependencies": {
    "@frontendmonster/utils": "0.3.11",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "lodash.has": "4.5.2",
    "read-pkg-up": "7.0.1"
  },
  "devDependencies": {
    "eslint": "7.26.0",
    "eslint-find-rules": "3.6.1",
    "husky": "5.2.0",
    "lint-staged": "10.5.4",
    "np": "7.4.0",
    "npm-run-all": "4.1.5",
    "pinst": "2.1.6",
    "prettier": "2.3.0",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "eslint": ">=7",
    "prettier": ">=1",
    "react": ">=16",
    "typescript": ">=4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}
