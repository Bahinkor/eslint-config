{
  "name": "@fullstacksjs/eslint-config",
  "version": "8.7.3",
  "license": "MIT",
  "author": "fullstacks <fullstacksjs@gmail.com>",
  "description": "fullstacks eslint config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fullstacksjs/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "fullstacksjs-eslint": "./bin/index.js"
  },
  "files": [
    "bin",
    "rules",
    "base.js",
    "index.js",
    "jest.js",
    "nextjs.js",
    "cypress.js",
    "storybook.js",
    "graphql.js",
    "react.js",
    "typescript.js",
    "typecheck.js",
    "esm.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "check-rules:base": "eslint-find-rules --unused ./base.js",
    "check-rules:jest": "eslint-find-rules --no-core --deprecated --unused ./tests/jest.js",
    "check-rules:react": "eslint-find-rules --no-core --unused ./tests/react.js",
    "check-rules:typescript": "eslint-find-rules --no-core --deprecated --unused ./tests/typescript.js",
    "check-rules:cypress": "eslint-find-rules --no-core --deprecated --unused ./tests/cypress.js",
    "check-rules:graphql": "eslint-find-rules --no-core --deprecated --unused ./tests/graphql.js",
    "check-rules:storybook": "eslint-find-rules --no-core --deprecated --unused ./tests/storybook.js",
    "spell": "cspell ./* --no-progress",
    "test": "npm-run-all --parallel check-rules:* && npm run spell",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pre-commit": "npm run test && lint-staged"
  },
  "main": "index.js",
  "dependencies": {
    "@frontendmonster/utils": "0.3.11",
    "@graphql-eslint/eslint-plugin": "3.10.7",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-plugin-react": "7.31.4",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-storybook": "0.6.4",
    "read-pkg-up": "7.0.1"
  },
  "devDependencies": {
    "cspell": "6.8.1",
    "eslint": "8.23.0",
    "eslint-find-rules": "4.1.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "np": "7.6.2",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "2.7.1",
    "typescript": "4.8.2"
  },
  "peerDependencies": {
    "cypress": ">=8",
    "eslint": ">=7",
    "graphql": ">=15",
    "prettier": "2",
    "react": ">=16",
    "typescript": "4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "cypress": {
      "optional": true
    },
    "jest": {
      "optional": true
    },
    "graphql": {
      "optional": true
    }
  }
}
